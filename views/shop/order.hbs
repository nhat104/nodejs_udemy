<main>
  {{#if hasOrder}}
    <ul class='orders'>
      {{#each orders}}
        <li class='orders__item'>
          <h1>{{_id}}</h1>
          <ul class='orders__products'>
            {{#each products}}
              <li class='orders__products-item'>
                {{title}}
                ({{quantity}})
              </li>
            {{/each}}
          </ul>
        </li>
      {{/each}}
    </ul>
  {{else}}
    <h1>Nothing there!</h1>
  {{/if}}
</main>